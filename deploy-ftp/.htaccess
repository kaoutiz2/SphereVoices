# Block access to sensitive files
<FilesMatch "^(\.env|\.git|composer\.(json|lock)|\.gitignore|README\.md|.*\.md)$">
    <IfModule mod_authz_core.c>
        Require all denied
    </IfModule>
    <IfModule !mod_authz_core.c>
        Order deny,allow
        Deny from all
    </IfModule>
</FilesMatch>

Options -Indexes
