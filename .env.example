# Database Configuration
# Copy this file to .env and fill in your database credentials

# Database Driver (mysql, pgsql, or sqlite)
DB_DRIVER=mysql

# Database Host
DB_HOST=localhost

# Database Port
DB_PORT=3306

# Database Name
DB_NAME=spherevoices_db

# Database Username
DB_USER=root

# Database Password
DB_PASSWORD=

# Database Prefix (optional, leave empty if not needed)
DB_PREFIX=

# Database Collation (for MySQL only)
DB_COLLATION=utf8mb4_general_ci
