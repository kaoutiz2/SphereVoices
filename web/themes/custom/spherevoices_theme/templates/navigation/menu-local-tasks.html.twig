 * Template personnalis√© pour les local tasks (liste des liens Voir, Modifier, etc.).
 */
#}
{% if primary %}
  <h2 class="visually-hidden">{{ 'Primary tabs'|t }}</h2>
  <ul{{ attributes.addClass('tabs', 'tabs--primary', 'action-links', 'action-links--tasks') }}>{{ primary }}</ul>
{% endif %}
{% if secondary %}
  <h2 class="visually-hidden">{{ 'Secondary tabs'|t }}</h2>
  <ul{{ attributes.addClass('tabs', 'tabs--secondary', 'action-links', 'action-links--tasks') }}>{{ secondary }}</ul>
{% endif %}

